<% def event_date_year(resultevent) %>
    <%	 Time::DATE_FORMATS[:year] = "%Y" %>
    <%	 return resultevent.start_date.in_time_zone("Central Time (US & Canada)").to_formatted_s(:year)  %>
<% end %>
<% def event_date_month(resultevent) %>
    <%	 Time::DATE_FORMATS[:month] = "%B" %>
    <%   if resultevent.start_date.in_time_zone("Central Time (US & Canada)") <= Date.tomorrow.in_time_zone("Central Time (US & Canada)") %>
    <% else %>
        <%	 return  t(resultevent.start_date.in_time_zone("Central Time (US & Canada)").to_formatted_s(:month))  %>
    <% end %>
<% end %>
<% def event_date_day(resultevent) %>
    <%	 Time::DATE_FORMATS[:day] = "%d" %>
    <%   if resultevent.start_date.in_time_zone("Central Time (US & Canada)") <= Date.tomorrow.in_time_zone("Central Time (US & Canada)") %>
         <%	 return t("location.today") %>
    <% else %>
    <%	 return resultevent.start_date.in_time_zone("Central Time (US & Canada)").to_formatted_s(:day)  %>
    <% end %>
<% end %>
<% def event_date_hour(resultevent) %>
    <%	 Time::DATE_FORMATS[:hour] = "%I:%M %P"%>
    <%	 return resultevent.start_date.in_time_zone("Central Time (US & Canada)").to_formatted_s(:hour)  %>
<% end %>
<% def event_name(resultevent) %>
    <%	 return  truncate(resultevent.name, :length => 33) %>
